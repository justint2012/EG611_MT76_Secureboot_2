+#!/bin/sh
+
+. partition_api.sh
+
+#gm reserved area
+factory_name="mfg"		
+factory_mtd=/dev/$(grep -i ''${factory_name}'' /proc/mtd | cut -c 1-4)
+
+#init seeds for generating JIO arguments
+
+usage()
+{
+	echo "This is a script to init JIO factory's data"
+	echo "Usage: $0 [ACTION] [OPTION] "
+	echo "ACTION: "
+	printf "\t%-16s	%s\n" "get" "get value from mfg reserved partition"
+	printf "\t%-16s	%s\n" "set" "set lan/wan MAC to mfg reserved partition"
+	echo "OPTION: "
+	printf "\t%-16s	%s\n" "wan_mac" "wan mac,eth1 mac"
+	printf "\t%-16s	%s\n" "sn" "board serial number"
+	printf "\t%-16s	%s\n" "rsn" "RJIL serial number"
+	printf "\t%-16s	%s\n" "wifi_ssid" "wifi ssid"
+	printf "\t%-16s	%s\n" "wifi_passwd" "wifi password"
+	#printf "\t%-16s	%s\n" "gpon_sn" "board GPON serial number"
+	printf "\t%-16s	%s\n" "acs_pre_passwd" "acs password"
+	printf "\t%-16s	%s\n" "hw_version" "hw version get from GPIO11,12"
+	printf "\t%-16s	%s\n" "uboot_passwd" "uboot password"
+	printf "\t%-16s	%s\n" "all" "show all arguments"
+	exit 1
+}
+
+usageGET()
+{
+	echo "Usage: $0 get [OPTION] "
+	echo " "
+	echo "OPTION: "
+	printf "\t%-16s	%s\n" "wan_mac" "get wan mac,eth1 mac"
+	printf "\t%-16s	%s\n" "sn" "board serial number"
+	printf "\t%-16s	%s\n" "rsn" "RJIL serial number"
+	printf "\t%-16s	%s\n" "wifi_ssid" "wifi ssid"
+	printf "\t%-16s	%s\n" "wifi_passwd" "wifi password"
+	#printf "\t%-16s	%s\n" "gpon_sn" "board GPON serial number"
+	printf "\t%-16s	%s\n" "acs_pre_passwd" "acs password"
+	printf "\t%-16s	%s\n" "hw_version" "hw version get from GPIO11,12"
+	printf "\t%-16s	%s\n" "uboot_passwd" "uboot password"
+	printf "\t%-16s	%s\n" "all" "show all arguments"
+	exit 1
+}
+
+usageSET()
+{
+	echo "Usage: $0 set [OPTION] "
+	echo " "
+	echo "OPTION: "
+	printf "\t%-8s	%s\n" "wan_mac" "wan mac,eth1 mac"
+	printf "\t%-8s	%s\n" ""        "ex: gm_factory_init.sh set wan_mac aa bb cc dd ee ff"
+	printf "\t%-8s	%s\n" "rsn"     "RJIL serial number"
+	printf "\t%-8s	%s\n" ""        "ex: gm_factory_init.sh set rsn RGMEGCJ12345678"
+	printf "\t%-8s	%s\n" "JIO"     "this command will get factory lan mac as seed "
+	printf "\t%-8s	%s\n" ""        "and use pwgen to create wifi_ssid,wifi_passwd,"
+	printf "\t%-8s	%s\n" ""        "sn,acs_pre_passwd,hw_version and uboot_passwd then write to mfg"
+	printf "\t%-8s	%s\n" ""        "ex: gm_factory_init.sh set JIO"
+	exit 1
+}
+
+#pwgen_by_seed [seed] [len]
+pwgen_by_seed()
+{
+	local seed=$1
+	local length=$2
+	
+	echo -n $seed > /tmp/input_seed
+	pwgen $length 1 -H /tmp/input_seed > /tmp/pwgen_output
+	cat /tmp/pwgen_output
+}
+
+if [ "$1" = "get" ]; then
+	if [ "$2" = "wan_mac" ] || [ "$2" = "sn" ] || [ "$2" = "rsn" ] ||
+	[ "$2" = "wifi_ssid" ] || [ "$2" = "wifi_passwd" ] ||
+	[ "$2" = "gpon_sn" ] || [ "$2" = "acs_pre_passwd" ] ||
+	[ "$2" = "hw_version" ] || [ "$2" = "uboot_passwd" ]; then
+		gm_get_factory $2
+	elif [ "$2" = "all" ]; then
+		echo -n "wan_mac           => "
+		gm_get_factory wan_mac
+		echo -n "sn                => "
+		gm_get_factory sn
+		echo -n "rsn               => "
+		gm_get_factory rsn
+		echo -n "wifi_ssid         => "
+		gm_get_factory wifi_ssid
+		echo -n "wifi_pw           => "
+		gm_get_factory wifi_passwd
+		#echo -n "gpon_sn           => "
+		#gm_get_factory gpon_sn
+		echo -n "acs_pre_password  => "
+		gm_get_factory acs_pre_passwd
+		echo -n "hw_version        => "
+		gm_get_factory hw_version
+		echo -n "uboot_passwd      => "
+		gm_get_factory uboot_passwd
+	else
+		usageGET
+		exit 1
+	fi
+elif [ "$1" = "set" ]; then
+	if [ "$2" = "wan_mac" ]; then
+		SetMac $2 $@
+	elif [ "$2" = "rsn" ]; then
+		gm_mfg_set_rsn $3
+	elif [ "$2" = "JIO" ]; then
+		gm_get_factory wan_mac
+		gm_get_factory sn
+		gm_get_factory rsn
+		gm_mfg_set_wifi_ssid
+		gm_mfg_set_wifi_pw
+		#gm_mfg_set_gpon_sn
+		gm_mfg_set_acs_pre_password
+		gm_mfg_set_hw_version
+		gm_mfg_set_uboot_passwd
+	else
+		usageSET
+		exit 1
+	fi
+else
+	usage
+	exit 1
+fi
