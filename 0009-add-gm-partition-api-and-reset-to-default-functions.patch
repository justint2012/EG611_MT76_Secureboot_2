+#!/bin/sh
+
+# Assign MAC address to variable
+MAC="$1"
+
+# Split MAC address into individual octets
+OCTET_1=${MAC:0:2}
+OCTET_2=${MAC:3:2}
+OCTET_3=${MAC:6:2}
+OCTET_4=${MAC:9:2}
+OCTET_5=${MAC:12:2}
+OCTET_6=${MAC:15:2}
+
+# Convert octets to decimal values
+DEC_1=$((0x${OCTET_1}))
+DEC_2=$((0x${OCTET_2}))
+DEC_3=$((0x${OCTET_3}))
+DEC_4=$((0x${OCTET_4}))
+DEC_5=$((0x${OCTET_5}))
+DEC_6=$((0x${OCTET_6}))
+
+# Increment the last octet and check for carry
+DEC_6=$((DEC_6 + 1))
+if [ $DEC_6 -gt 255 ]; then
+  DEC_6=0
+  DEC_5=$((DEC_5 + 1))
+  if [ $DEC_5 -gt 255 ]; then
+	DEC_5=0
+	DEC_4=$((DEC_4 + 1))
+	if [ $DEC_4 -gt 255 ]; then
+	  DEC_4=0
+	  DEC_3=$((DEC_3 + 1))
+	  if [ $DEC_3 -gt 255 ]; then
+		DEC_3=0
+		DEC_2=$((DEC_2 + 1))
+		if [ $DEC_2 -gt 255 ]; then
+		  DEC_2=0
+		  DEC_1=$((DEC_1 + 1))
+		  if [ $DEC_1 -gt 255 ]; then
+			DEC_1=0
+		  fi
+		fi
+	  fi
+	fi
+  fi
+fi
+
+# Convert decimal values back to hex
+OCTET_1=$(printf "%02x" $DEC_1)
+OCTET_2=$(printf "%02x" $DEC_2)
+OCTET_3=$(printf "%02x" $DEC_3)
+OCTET_4=$(printf "%02x" $DEC_4)
+OCTET_5=$(printf "%02x" $DEC_5)
+OCTET_6=$(printf "%02x" $DEC_6)
+
+# Combine octets back into a MAC address
+NEW_MAC="${OCTET_1}:${OCTET_2}:${OCTET_3}:${OCTET_4}:${OCTET_5}:${OCTET_6}"
+
+echo "$NEW_MAC"
\ No newline at end of file
