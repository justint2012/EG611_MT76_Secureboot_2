+led_path="/sys/devices/platform/leds/leds"
+
+##
+# $1 - red on/off
+# $2 - green on/off
+# $3 - blue on/off
+##
+rgb_onoff()
+{
+        echo none > $led_path/led_r/trigger
+        echo none > $led_path/led_g/trigger
+        echo none > $led_path/led_b/trigger
+        echo $1 > $led_path/led_r/brightness
+        echo $2 > $led_path/led_g/brightness
+        echo $3 > $led_path/led_b/brightness
+}
+
+##
+# $1 - red on/off
+# $2 - green on/off
+# $3 - blue on/off
+# $4 - blink intervel ms(default 500)
+##
+rgb_blink()
+{
+        echo 0 > $led_path/led_r/brightness
+        echo 0 > $led_path/led_g/brightness
+        echo 0 > $led_path/led_b/brightness
+        echo timer > $led_path/led_r/trigger
+        echo timer > $led_path/led_g/trigger
+        echo timer > $led_path/led_b/trigger
+        if [ -z $4 ]; then
+                delay="500"
+        else
+                delay=$4
+        fi
+        echo $delay > $led_path/led_r/delay_off
+        echo $delay > $led_path/led_r/delay_on
+        echo $delay > $led_path/led_g/delay_off
+        echo $delay > $led_path/led_g/delay_on
+        echo $delay > $led_path/led_b/delay_off
+        echo $delay > $led_path/led_b/delay_on
+        echo $1 > $led_path/led_r/brightness
+        echo $2 > $led_path/led_g/brightness
+        echo $3 > $led_path/led_b/brightness
+}
+
+usage_print()
+{
+	echo "$0 <mode>"
+	echo "mode: r, g, b, all, none, blink"
+}
+
+if [ -z $1 ]; then
+	usage_print
+	exit
+fi
+
+case $1 in
+	r)
+		rgb_onoff 1 0 0
+	;;
+	g)
+		rgb_onoff 0 1 0
+	;;
+	b)
+		rgb_onoff 0 0 1
+	;;
+	all)
+		rgb_onoff 1 1 1
+	;;
+	none)
+		rgb_onoff 0 0 0
+	;;
+	blink)
+		rgb_blink 1 1 1
+	;;
+	*)
+		usage_print
+esac
