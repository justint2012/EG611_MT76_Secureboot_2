+#!/bin/sh
+
+#
+#Define JIO factory mfg partition offset
+#   default             length(Bytes)    offset
+# 1.wan_mac          :    6               0x00    //6 bytes mac
+# 2.sn               :    8               0x06    //8 bytes
+# 3.rsn              :    15              0x0e    //7-char + 8 digits = 7+8bytes = 15 bytes
+# 4.wifi_SSID        :    15              0x1d    //"JioFiber-"+6 char = 15 char = 15 byte
+# 5.wifi_passwd      :    16              0x2c    //16-character random password = 16 bytes
+# 6.gpon_sn          :    12              0x3c    //4-character “Vendor ID”+4 bytes mac = 4+8 bytes =12
+# 7.acs_pre_password :    16              0x48    //16-character random password = 16 bytes
+# 8.hw_version       :    2               0x58    //2-charactor = 2 bytes
+# 9.uboot_passwd     :    16              0x5a    //16-char = 16 bytes
+#
+WAN_MAC_OFFSET=0x00
+SN_OFFSET=0x06
+RSN_OFFSET=0x0e
+WIFI_SSID_OFFSET=0x1d
+WIFI_PASSWD_OFFSET=0x2c
+GPON_SN_OFFSET=0x3c
+ACS_PRE_PASSWD_OFFSET=0x48
+HW_VERSION_OFFSET=0x58
+UBOOT_PASSWD_OFFSET=0x5a
+
+
+WAN_MAC_MAX_LEN=6
+SN_MAX_LEN=8
+RSN_MAX_LEN=15
+WIFI_SSID_MAX_LEN=15
+WIFI_PASSWD_MAX_LEN=16
+GPON_SN_MAX_LEN=12
+ACS_PRE_PASSWD_MAX_LEN=16
+HW_VERSION_MAX_LEN=2
+UBOOT_PASSWD_MAX_LEN=16
+
+##################### MTK function #############################
+
+#1.Read the offset's data from the Factory
+#usage: Get_offset_data length offset
+Get_offset_data()
+{
+	local length=$1
+	local offset=$2
+
+	hexdump -v -n ${length} -s ${offset} -e ''`expr ${length} - 1`'/1 "%02x-" "%02x "' ${factory_mtd}
+}
+
+#2.Write the offset's data from the Factory
+#usage: Set_offset_data length offset data
+Set_offset_data()
+{
+	local length=$1
+	local offset=$2
+	local index=`expr $# - ${length} + 1`
+	local data=""
+	
+	for j in $(seq ${index} `expr ${length} + ${index} - 1`)
+	do
+		temp=`eval echo '$'{"$j"}`
+		data=${data}"\x${temp}"
+	done
+
+	dd if=${factory_mtd} of=/tmp/Factory.backup
+	printf "${data}" | dd conv=notrunc of=/tmp/Factory.backup bs=1 seek=$((${offset}))
+	mtd write /tmp/Factory.backup ${factory_name}
+	rm -rf /tmp/Factory.backup
+}
+
+SetMac()
+{
+	if [ "$#" != "9" ]; then
+		echo "Mac address must be 6 bytes!"
+		exit 1
+	fi
+
+	if [ "$1" == "wan_mac" ]; then
+		#write wan mac
+		Set_offset_data 6 ${WAN_MAC_OFFSET} $@
+	fi
+}
+
+get_random_string()
+{
+	local seed=$1
+	local length=$2
+	
+	echo -n $seed > /tmp/input_seed
+	pwgen $length 1 -H /tmp/input_seed > /tmp/pwgen_output
+	cat /tmp/pwgen_output
+}
+
+##################### get arguments from mfg#############################
+
+gm_mfg_get_val()
+{
+	HEX_STR=`Get_offset_data ${1} ${2} | sed 's/-/ \\\x/g'`
+	echo -e "\x${HEX_STR}" | sed 's/ //g'
+}
+
+# length should be 8 digits = 8 bytes
+gm_mfg_get_sn()
+{
+	SN_STR=`Get_offset_data ${SN_MAX_LEN} ${SN_OFFSET} | sed 's/-/ \\\x/g'`
+	echo -e "\x${SN_STR}" | sed 's/ //g'
+}
+
+# RJIL Identifier – R
+# Device Vendor Identifier – AA
+# Device Type Identifier – OD
+# Month of Manufacturing – G (July-refer above table)
+# Year of Manufacturing – A (2014-refer above table)
+# Serial Number – 12345678
+# length should be 7 + 8 =15 characters =15 bytes
+gm_mfg_get_rsn()
+{
+	RSN_HEX_STR=`Get_offset_data ${RSN_MAX_LEN} ${RSN_OFFSET} | sed 's/-/ \\\x/g'`
+	echo -e "\x${RSN_HEX_STR}" | sed 's/ //g'
+}
+
+gm_mfg_get_wifi_ssid()
+{
+	HEX_STR=`Get_offset_data ${WIFI_SSID_MAX_LEN} ${WIFI_SSID_OFFSET} | sed 's/-/ \\\x/g'`
+	echo -e "\x${HEX_STR}" | sed 's/ //g'
+}
+
+gm_mfg_get_wifi_passwd()
+{
+	HEX_STR=`Get_offset_data ${WIFI_PASSWD_MAX_LEN} ${WIFI_PASSWD_OFFSET} | sed 's/-/ \\\x/g'`
+	echo -e "\x${HEX_STR}" | sed 's/ //g'
+}
+
+gm_mfg_get_acs_pre_passwd()
+{
+	HEX_STR=`Get_offset_data ${ACS_PRE_PASSWD_MAX_LEN} ${ACS_PRE_PASSWD_OFFSET} | sed 's/-/ \\\x/g'`
+	echo -e "\x${HEX_STR}" | sed 's/ //g'
+}
+
+gm_mfg_get_gpon_sn()
+{
+	HEX_STR=`Get_offset_data ${GPON_SN_MAX_LEN} ${GPON_SN_OFFSET} | sed 's/-/ \\\x/g'`
+	echo -e "\x${HEX_STR}" | sed 's/ //g'
+}
+
+gm_mfg_get_hw_version()
+{
+	HEX_STR=`Get_offset_data ${HW_VERSION_MAX_LEN} ${HW_VERSION_OFFSET} | sed 's/-/ \\\x/g'`
+	echo -e "\x${HEX_STR}" | sed 's/ //g'
+}
+
+gm_mfg_get_uboot_passwd()
+{
+	HEX_STR=`Get_offset_data ${UBOOT_PASSWD_MAX_LEN} ${UBOOT_PASSWD_OFFSET} | sed 's/-/ \\\x/g'`
+	echo -e "\x${HEX_STR}" | sed 's/ //g'
+}
+
+gm_get_factory()
+{
+	if [ "$1" = "wan_mac" ]; then
+		Get_offset_data 6 ${WAN_MAC_OFFSET} | sed 's/-/:/g'
+		echo ""
+	elif [ "$1" = "sn" ]; then
+		gm_mfg_get_val ${SN_MAX_LEN} ${SN_OFFSET}
+	elif [ "$1" = "rsn" ]; then
+		gm_mfg_get_val ${RSN_MAX_LEN} ${RSN_OFFSET}
+	elif [ "$1" = "wifi_ssid" ]; then
+		gm_mfg_get_val ${WIFI_SSID_MAX_LEN} ${WIFI_SSID_OFFSET}
+	elif [ "$1" = "wifi_passwd" ]; then
+		gm_mfg_get_val ${WIFI_PASSWD_MAX_LEN} ${WIFI_PASSWD_OFFSET}
+	elif [ "$1" = "gpon_sn" ]; then
+		gm_mfg_get_val ${GPON_SN_MAX_LEN} ${GPON_SN_OFFSET}
+	elif [ "$1" = "acs_pre_passwd" ]; then
+		gm_mfg_get_val ${ACS_PRE_PASSWD_MAX_LEN} ${ACS_PRE_PASSWD_OFFSET}
+	elif [ "$1" = "hw_version" ]; then
+		gm_mfg_get_val ${HW_VERSION_MAX_LEN} ${HW_VERSION_OFFSET}
+	elif [ "$1" = "uboot_passwd" ]; then
+		gm_mfg_get_val ${UBOOT_PASSWD_MAX_LEN} ${UBOOT_PASSWD_OFFSET}
+	else
+		echo "unknown value ${1}"
+		exit 1
+	fi
+}
+
+##################### set arguments to mfg #############################
+
+ACS_SEED="foxconnRJIL"
+#uboot random string created by "openssl rand -base64 8|cut -c 1-8"
+#this value will be update at every SKU
+UBOOT_PASSWD_RANDOM_STR_JIDU6801="9rOL8bjr"
+
+gm_mfg_set_sn()
+{
+	local input_str=$1
+	SN_HEX=`echo "${input_str}" | hexdump -C|head -n 1|cut -d ' ' -f 3-11`
+	Set_offset_data ${SN_MAX_LEN} ${SN_OFFSET} ${SN_HEX}
+	echo "init_sn => $(gm_mfg_get_sn)"
+	echo "====================================="
+}
+
+gm_mfg_set_rsn()
+{
+	local input_str=$1
+	SN_STR=`echo "${input_str}" | cut -c 8-15`
+	gm_mfg_set_sn ${SN_STR}
+	RSN_HEX=`echo "${input_str}" | hexdump -C|head -n 1|cut -d ' ' -f 3-18`
+	Set_offset_data ${RSN_MAX_LEN} ${RSN_OFFSET} ${RSN_HEX}
+	echo "init_rsn => $(gm_mfg_get_rsn)"
+	echo "====================================="
+}
+
+gm_mfg_set_wifi_ssid()
+{
+	BASE_MAC=`Get_offset_data ${WAN_MAC_MAX_LEN} ${WAN_MAC_OFFSET} | sed 's/-//g'`
+	wifi_random_SSID=`get_random_string ${BASE_MAC} 6`
+	wifi_SSID="JioFiber-${wifi_random_SSID}"
+	wifi_SSID_HEX=`echo "${wifi_SSID}" | hexdump -C|head -n 1|cut -d ' ' -f 3-18`
+	Set_offset_data ${#wifi_SSID} ${WIFI_SSID_OFFSET} ${wifi_SSID_HEX}
+	echo "init_wifi_ssid => $(gm_mfg_get_wifi_ssid)"
+	echo "====================================="
+}
+
+#use SN as the seed to generate random wifi password
+gm_mfg_set_wifi_pw()
+{
+	wifi_random_passwd=`get_random_string $(gm_mfg_get_sn) 16`
+	wifi_random_HEX=`echo "${wifi_random_passwd}" | hexdump -C|head -n 1|cut -d ' ' -f 3-19`
+	Set_offset_data ${#wifi_random_passwd} ${WIFI_PASSWD_OFFSET} ${wifi_random_HEX}
+	echo "init_wifi_pw => $(gm_mfg_get_wifi_passwd)"
+	echo "====================================="
+}
+
+gm_mfg_set_gpon_sn()
+{
+	JIO_VENDOR="RJIL"
+	LAST_4BYTE_MAC=`Get_offset_data ${WAN_MAC_MAX_LEN} ${WAN_MAC_OFFSET} | sed 's/-/ /g' | cut -d ' ' -f 3-6| sed 's/ //g'`
+	GPON_SN_STR="${JIO_VENDOR}${LAST_4BYTE_MAC}"
+	GPON_SN_HEX=`echo "${GPON_SN_STR}" | hexdump -C|head -n 1|cut -d ' ' -f 3-15`
+	Set_offset_data ${GPON_SN_MAX_LEN} ${GPON_SN_OFFSET} ${GPON_SN_HEX}
+	echo "init_sn => $(gm_mfg_get_gpon_sn)"
+	echo "====================================="
+}
+
+#use "foxconnRJIL" as the seed to generate acs pre-password
+gm_mfg_set_acs_pre_password()
+{
+	acs_pre_password=`get_random_string ${ACS_SEED} 16`
+	acs_pre_password_HEX=`echo "${acs_pre_password}" | hexdump -C|head -n 1|cut -d ' ' -f 3-19`
+	Set_offset_data ${ACS_PRE_PASSWD_MAX_LEN} ${ACS_PRE_PASSWD_OFFSET} ${acs_pre_password_HEX}
+	echo "init_acs_pre_password => $(gm_mfg_get_acs_pre_passwd)"
+	echo "====================================="
+}
+
+gm_mfg_set_hw_version()
+{
+	GPIO11=`gpioget gpiochip0 16`
+	GPIO12=`gpioget gpiochip0 17`
+	
+	HW_VERSION=0
+	
+	if [ "$GPIO12" = "1" ]; then
+		HW_VERSION=`expr $HW_VERSION + 2`
+	fi
+	
+	if [ "$GPIO11" = "1" ]; then
+		HW_VERSION=`expr $HW_VERSION + 1`
+	fi
+	
+	HW_VERSION_STR="S${HW_VERSION}"
+	HW_VERSION_STR_HEX=`echo "${HW_VERSION_STR}" | hexdump -C|head -n 1|cut -d ' ' -f 3-4`
+	Set_offset_data ${HW_VERSION_MAX_LEN} ${HW_VERSION_OFFSET} ${HW_VERSION_STR_HEX}
+	echo "init_hw_version => $(gm_mfg_get_hw_version)"
+	echo "====================================="
+}
+
+gm_mfg_set_uboot_passwd()
+{
+	SN_STR=`gm_mfg_get_sn`
+	#UBOOT_PASSWD_RANDOM_STR=`openssl rand -base64 8|cut -c 1-8`
+	SPLIT_SN_STR=`echo "${SN_STR}"| sed 's/\(.\)/\1 /g'`
+	reverse_sn=
+	for i in ${SPLIT_SN_STR}
+	do
+		reverse_sn=${i}${reverse_sn}
+	done
+	UBOOT_PASSWD_STR="${reverse_sn}${UBOOT_PASSWD_RANDOM_STR_JIDU6801}"
+	UBOOT_PASSWD_STR_HEX=`echo "${UBOOT_PASSWD_STR}" | hexdump -C|head -n 1|cut -d ' ' -f 3-19`
+	Set_offset_data ${UBOOT_PASSWD_MAX_LEN} ${UBOOT_PASSWD_OFFSET} ${UBOOT_PASSWD_STR_HEX}
+	echo "random string => ${UBOOT_PASSWD_RANDOM_STR_JIDU6801}"
+	echo "init_uboot_passwd => $(gm_mfg_get_uboot_passwd)"
+	echo "====================================="
+}
